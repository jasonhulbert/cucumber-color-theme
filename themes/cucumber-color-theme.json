  "tokenColors": [
    {
      "scope": ["meta.brace", "meta.punctuation"],
      "settings": {
        "foreground": "#b3c2bf"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#686463"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#4e97d3"
      }
    },
    {
      "scope": "entity.name.operator",
      "settings": {
        "foreground": "#3dacc2"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#9cd5d5"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#4e97d3"
      }
    },
    {
      "scope": [
        "variable.other.readonly",
        "variable.other.readonly.property",
        "variable.other.constant",
        "variable.other.constant.property",
        "variable.other.object",
        "variable.other.object.property",
        "entity.name.enum",
        "entity.name.enum.member"
      ],
      "settings": {
        "foreground": "#44c0d6"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#c98777"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#9b7ee4"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#daf1f1"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#c98777"
      }
    },
    {
      "scope": [
        "meta.punctuation.template-expression",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#c98777"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#4e97d3"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.class.css",
        "source.css entity.other.attribute-name.class",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.scss",
        "source.css entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#2ebab8"
      }
    },
    {
      "scope": ["support.type", "entity.name.type", "entity.other.attribute"],
      "settings": {
        "foreground": "#2ebab8"
      }
    },
    {
      "scope": [
        "support.class",
        "entity.name.type.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#2ebab8"
      }
    },
    {
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#2ebab8"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#9cd5d5"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#aecb8f"
      }
    },
    {
      "scope": ["punctuation.decorator"],
      "settings": {
        "foreground": "#aecb8f"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#daf1f1"
      }
    },
    {
      "scope": "markup.heading.markdown",
      "settings": {
        "foreground": "#2ebab8"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#4e97d3"
      }
    },
    {
      "scope": [
        "punctuation.definition.link.title.begin.markdown",
        "punctuation.definition.link.title.end.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.end.markdown",
        "punctuation.definition.metadata.markdown"
      ],
      "settings": {
        "foreground": "#44c0d6"
      }
    },
    {
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#2ebab8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#b3c2bf"
      }
    }
  ]
}
